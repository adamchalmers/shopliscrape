<script type="text/javascript">
(function(){
	var onlineUrls = {
		'recipe': 'http://9nl.be/a2nt',
		'howto': 'http://9nl.be/j191',
		'foodfeatures': 'http://9nl.be/jr9c',
		'community': 'http://9nl.be/0otx',
		'magazine': 'http://9nl.be/yyeg',
		'win': 'http://9nl.be/q0s3'
	};

	var storeUrls = {
		'recipe': 'http://9nl.be/tc6t',
		'howto': 'http://9nl.be/q0yq',
		'foodfeatures': 'http://9nl.be/k7gw',
		'community': 'http://9nl.be/q2vd',
		'magazine': 'http://9nl.be/zjf7',
		'win': 'http://9nl.be/xmd1'
	};

	var body = $('body');
	var type = 'recipe';
	if(body.hasClass('magazine')) {
		type = 'magazine';
	} else {
		switch(body.attr('id')) {
			case 'how_to_selected':
			case 'taste_tv_selected':
				type = 'howto';
				break;

			case 'news_selected':
				type = 'foodfeatures';
				break;

			case 'members_selected':
			case 'forums_selected':
				type = 'community';
				break;

			case 'win_selected':
				type = 'win';
				break;

			case 'recipes_selected':
			case 'glossary_selected':
			default:
				type = 'recipe';
				break;
		}
	}

	$('#coles-online-link').attr('href', onlineUrls[type]);
	$('#coles-stores-link').attr('href', storeUrls[type]);
})();
</script><div class="module food-hub" data-external="false">
