<script type="text/javascript">
/* <![CDATA[ */
	foreignId = 18338;
	assetType = 'Recipe';
	mealPlanId = 0;
	// Hide all reply forms on pageload
	$(".reply").hide();
	$("#questionTab").hide();
	$("#noteTab").hide();
	
	/**
	 * Tabbing functions for the comment section of the page
	 */
	$("a#ratings, a#ratethis").live('click', function(){
		$("a#ratings").addClass("active");
		$("a#questions").removeClass("active");
		$("a#mynotes").removeClass("active");
		$("div#commentTab").show();
		$("div#questionTab").hide();
		$("div#noteTab").hide();
	});
	$("a#jumpToQuestions,a#askquest,a#questions").live('click', function(){
		$("a#questions").addClass("active");
		$("a#ratings").removeClass("active");
		$("a#mynotes").removeClass("active");
		$("div#questionTab").show();
		$("div#commentTab").hide();
		$("div#noteTab").hide();
	});
	$("a#mynotes,a#addnotes").live('click', function(){
		// AJAX-load the contents for notes (URL defined in /recipe/comments.phtml)
		$("#noteTabContent").load(noteUrl,function(){
			// hide reply boxes once loaded.
			$('div[id^=noteForm]').hide();
		});
		$("a#mynotes").addClass("active");
		$("a#ratings").removeClass("active");
		$("a#questions").removeClass("active");
		$("div#noteTab").show();
		$("div#commentTab").hide();
		$("div#questionTab").hide();
	});
		$(window).resize(function(){
		if ($(window).width() < 700) {
			$("#jqmWindowComment").css({ position: "absolute", left: $(window).scrollLeft() + 300, top: $(window).scrollTop() + 200 });
		}
		else if (!$.browser.msie || ($.browser.msie && $.browser.version > 6)) {
			$("#jqmWindowComment").css({ position: "fixed", left: "50%", top: "50%" });
		}
	});
/* ]]> */
</script>
